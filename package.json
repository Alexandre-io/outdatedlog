{
  "name": "outdatedlog",
  "version": "3.2.1",
  "description": "This command will check the registry to see if any (or, specific) installed packages are currently outdated and print the changelog",
  "main": "lib/outdatedlog",
  "scripts": {
    "test": "snyk test && node_modules/jshint/bin/jshint ./",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "npm",
    "outdated",
    "changelog",
    "change log"
  ],
  "preferGlobal": true,
  "author": "Alexandre L.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexandre-io/outdatedlog.git"
  },
  "bugs": {
    "url": "http://github.com/Alexandre-io/outdatedlog/issues"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "bin": {
    "outdatedlog": "./bin/outdatedlog.js"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "license": "MIT",
  "dependencies": {
    "async": "~3.2.0",
    "chalk": "~4.0.0",
    "changelog": "~1.4.2",
    "cli": "~1.0.1",
    "npm": "~6.14.5",
    "semver-compare": "~1.0.0",
    "snyk": "^1.327.0"
  },
  "devDependencies": {
    "jshint": "~2.11.1"
  },
  "snyk": true
}
