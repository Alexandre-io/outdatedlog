{
  "name": "outdatedlog",
  "version": "1.0.0",
  "description": "This command will check the registry to see if any (or, specific) installed packages are currently outdated and print the changelog",
  "main": "lib/outdatedlog",
  "scripts": {
    "test": "node_modules/jshint/bin/jshint ./"
  },
  "keywords": [
    "npm",
    "outdated",
    "changelog",
    "change log"
  ],
  "preferGlobal": true,
  "author": "Alexandre L.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexandre-io/outdatedlog.git"
  },
  "bugs": {
    "url": "http://github.com/Alexandre-io/outdatedlog/issues"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "bin": {
    "outdatedlog": "./bin/outdatedlog.js"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^1.2.1",
    "chalk": "^1.0.0",
    "changelog": "^1.0.7",
    "cli": "^0.9.0",
    "npm": "^2.13.3",
    "semver-compare": "^1.0.0"
  },
  "devDependencies": {
    "jshint": "^2.8.0"
  }
}
